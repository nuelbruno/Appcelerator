<Alloy>
	<Window id="winHome" class="win" onOpen="winOpen" onFocus="winFocus">

		<View id="leftView" class="leftPanelProperty">
			<Require src="common/viewLeftPanel" id="viewLeftPanel"></Require>
		</View>
		<View id="mainView" class="mainContainer">
			<View id="navBarBackView" class="navBarBackViewProperty">
				<View id="statusBar" class="statusBarProperty"></View>
				<View class="navBarProperty">
					<!--<View class="backView" onClick="closeWindow">
						<ImageView class="imgBackBtn"></ImageView>
					</View>
					<Label id="lblNavTitle" class="lblNavTitleProperty"></Label>-->
					
					<Require src="common/viewNavigationTools" id="viewNavigationTools"></Require>
				</View>
			</View>
			<View id="menuView">
				<View id="viewWeather">
					<ImageView id="imageViewWeather"></ImageView>
					<View id="viewWeatherDetail">
						<Label id="labelWeatherCity"></Label>
						<View id="viewDetail">
							<Label id="labelDegree"></Label>
							<Label id="labelLine"></Label>
							<Label id="labelWeatherType"></Label>
						</View>
					</View>
				</View>
				<View id="scrollingView">
					<View id="viewAbout" onClick="openAbout">
						<ImageView id="imageViewAbout" class="imgProperty"></ImageView>
						<Label id="labelAbout" class="lblProperty"></Label>
						<ImageView id="imageViewAboutArrow"></ImageView>
					</View>
					<View id="viewNews" onClick="openNewsList">
						<View id="viewIconContainerNews" class="classViewIconContainer" visible="false">
							<Label id="lblNewsBadgeCounter" class="classLabelNotificationCounter"></Label>
						</View>
						<ImageView id="imageViewNews" class="imgProperty"></ImageView>
						<Label id="labelNews" class="lblProperty"></Label>
						<ImageView id="imageViewNewsArrow"></ImageView>
					</View>
					<View id="viewServices" onClick="openServices">
						<View id="viewIconContainerEservices" class="classViewIconContainer" visible="false">
							<Label id="labelNotificationCounterEservices" class="classLabelNotificationCounter"></Label>
						</View>
						<ImageView id="imageViewServices" class="imgProperty"></ImageView>
						<Label id="labelServices" class="lblProperty"></Label>
						<ImageView id="imageViewServicesArrow"></ImageView>
					</View>
					<View id="viewEvents" onClick="openEvents">
						<View id="viewIconContainerEvents" class="classViewIconContainer" visible="false">
							<!-- <Label id="labelNotificationCounterEvents" class="classLabelNotificationCounter"></Label> -->
							<Label id="lblEventBadgeCounter" class="classBadgeCounterlabel"></Label>
						</View>
						<ImageView id="imageViewEvents" class="imgProperty"></ImageView>
						<Label id="labelEvents" class="lblProperty"></Label>
						<ImageView id="imageViewEventsArrow"></ImageView>
					</View>
					<View id="viewFeedback" onClick="openFeedback">
						<ImageView id="imageViewFeedback" class="imgProperty"></ImageView>
						<Label id="labelFeedback" class="lblProperty"></Label>
						<ImageView id="imageViewFeedbackArrow"></ImageView>
					</View>
					<View id="viewFuneral" onClick="openUaqFunerals">
						<View id="viewIconContainerFunerals" class="classViewIconContainer" visible="false">
							<!-- <Label id="labelNotificationCounterFunerals" class="classLabelNotificationCounter"></Label> -->
							<Label id="lblFuneralBadgeCounter" class="classBadgeCounterlabel"></Label>
						</View>
						<ImageView id="imageViewFuneral" class="imgProperty"></ImageView>
						<Label id="labelFuneral" class="lblProperty"></Label>
						<ImageView id="imageViewFuneralArrow"></ImageView>
					</View>
					<View id="viewContact" onClick="openContactUs">
						<ImageView id="imageViewContact" class="imgProperty"></ImageView>
						<Label id="labelContact" class="lblProperty"></Label>
						<ImageView id="imageViewContactArrow"></ImageView>
					</View>
				</View>
				
			</View>
		</View>
		<Require src="common/viewHappinessIndicator" id="viewHappinessIndicator"></Require>
		<Require src="common/viewNotification" id="viewNotification"></Require>
		<View id="viewTransparent" class="transparentViewProperty" onClick="closeLeftPanel"></View>
	</Window>
</Alloy>