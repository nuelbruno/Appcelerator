<Alloy>
	<ItemTemplate name="itemTemplateNews" id="itemTemplateNews">
		<View id="viewNewsRow" bindId="viewNewsRow">
			<ImageView id="imageviewNewsDetail" bindId="imageviewDetail"></ImageView>
			<View id="viewDetail" bindId="viewDetail">
				<View height="5" borderColor="transparent" top="0" left = "0" right = "0" backgroundColor="transparent"></View>
				<Label id="labelNewsTitle" bindId="labelTitle"></Label>
				<ImageView id="imageviewSeparator"></ImageView>
				<View id="viewNewsDate">
					<ImageView id="imageviewDateIcon" bindId="imageviewDateIcon"></ImageView>
					<Label id="labelDate" bindId="labelDate"></Label>
				</View>
				<View height="15" borderColor="transparent" left = "0" bottom="0" right = "0" backgroundColor="transparent"></View>
			</View>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateEvents" id="itemTemplateEvents">
		<View id="viewEventRow" bindId="viewEventRow">
			<ImageView id="imageviewEventDetail" bindId="imageviewDetail"></ImageView>
			<View id="viewDetail" bindId="viewDetail">
				<Label id="labelEventTitle" bindId="labelTitle"></Label>
				<ImageView id="imageviewSeparator"></ImageView>
				<View id="viewLocation">
					<ImageView id="imageviewLocationIcon" bindId="imageviewLocationIcon"></ImageView>
					<Label id="labelLocation" bindId="labelLocation"></Label>
				</View>
				<View id="viewEventDate">
					<ImageView id="imageviewDateIcon" bindId="imageviewDateIcon"></ImageView>
					<Label id="labelDate" bindId="labelDate"></Label>
				</View>
			</View>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateDepartment" id="itemTemplateDepartment">
		<View id="viewDepartmentRow" bindId="viewDepartmentRow">
			<ImageView id="imageviewDepartment" bindId="imageviewDepartment"></ImageView>
			<View id="viewDepartmentDetail" bindId="viewDepartmentDetail">
				<Label id="labelTitle" bindId="labelTitle"></Label>
				<Label id="labelDescription" bindId="labelDescription"></Label>
				<ImageView id="imageviewSeparator"></ImageView>
				<View id="viewButtons" bindId="viewButtons">
					<ImageView id="imageviewCall" bindId="imageviewCall" class="buttonProperty"></ImageView>
					<ImageView id="imageviewFax" bindId="imageviewFax" class="buttonProperty"></ImageView>
					<ImageView id="imageviewMail" bindId="imageviewMail" class="buttonProperty"></ImageView>
					<ImageView id="imageviewWeb" bindId="imageviewWeb" class="buttonProperty"></ImageView>
					<ImageView id="imageviewFacebook" bindId="imageviewFacebook" class="buttonProperty"></ImageView>
					<ImageView id="imageviewTwitter" bindId="imageviewTwitter" class="buttonProperty"></ImageView>
				</View>
			</View>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateFavourite" id="itemTemplateFavourite">
		<View id="viewFavouriteRow" bindId="viewFavouriteRow">
			<!-- <ImageView id="imageviewTopSeparator"></ImageView> -->
			<Label id="labelFavouriteTitle" bindId="labelFavouriteTitle"></Label>
			<ImageView id="imageviewDelete" bindId="imageviewDelete"></ImageView>
			<!-- <ImageView id="imageviewBottomSeparator"></ImageView> -->
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateNotification" id="itemTemplateNotification">
		<View id="viewNotificationRow" bindId="viewFavouriteRow">
			<View id="viewTitle">
				<Label id="labelNotificationTitle" bindId="labelNotificationTitle"></Label>
				<Label id="labelNofificationTime" bindId="labelNofificationTime"></Label>
			</View>
			<Label id="labelNotificationDescription" bindId="labelNotificationDescription"></Label>
			<ImageView id="imageviewNotificationSeparator" bindId="imageviewNotificationSeparator"></ImageView>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateServices" id="itemTemplateServices">
		<View id="viewServicesRow" bindId="viewServicesRow">
			<!-- <View id="viewGreenIcon" bindId="viewGreenIcon"></View> -->
			<Label id="labelServicesLIVE" bindId="labelServicesLIVE"></Label>
			<Label id="labelServicesTitle" bindId="labelServicesTitle"></Label>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateFuneral" id="itemTemplateFuneral">
		<View id="viewFuneralRow" bindId="viewFuneralRow" layout="vertical">
			<Label id="labelFuneralTitle" bindId="labelFuneralTitle"></Label>			
		</View>
		<View id="viewFuneralBottomRow" ></View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateMyrequestDropdown" id="itemTemplateMyrequestDropdown">
		<View id="viewMyrequestRow" bindId="viewMyrequestRow" layout="vertical">
			<Label id="labelMyRequestTitle" bindId="labelMyRequestTitle"></Label>
		</View>
	</ItemTemplate>
	<ItemTemplate name="itemTemplateMyservice" id="itemTemplateMyservice">
		<View id="viewMyRequestRow" bindId="viewMyRequestRow">
			<View id="viewTitleMyrequest">
				<Label id="labelMyRequestTitle" bindId="labelMyRequestTitle"></Label>
			</View>
			<Label id="labelMyRequestDate" bindId="labelMyRequestDate"></Label>
			<Label id="labelMyRequestDescription" bindId="labelMyRequestDescription"></Label>
			<ImageView id="imageviewMyRequestSeparator" bindId="imageviewMyRequestSeparator"></ImageView>
		</View>
	</ItemTemplate>
	
	<ItemTemplate name="more" id="more">
		<View id="viewMore" bindId="viewMore">
			<Label id="viewMoreLabel" bindId="viewMoreLabel"></Label>
		</View>
	</ItemTemplate>
	
	<ItemTemplate name="itemTemplatePaymentHistory" id="itemTemplatePaymentHistory">
		<View id="viewPaymentHistoryRow" bindId="viewPaymentHistoryRow" layout="vertical">
			<Label id="labelPaymentHistoryTitle" bindId="labelPaymentHistoryTitle"></Label>	
		</View>
		<View id="viewFuneralBottomRow" ></View>
	</ItemTemplate>
	
	
