	top : 0,
}

"#webView[formFactor=handheld]":{
	top : 0,
	bottom : 50,
	width : "100%",
	backgroundColor : "white",
	
}
"#webView[platform=android]":{
	cacheMode : Titanium.UI.Android.WEBVIEW_LOAD_NO_CACHE	
}

"#webView[formFactor=tablet]":{
	top : 0,
	bottom : 65,
	width : "100%",
	backgroundColor : "white",
}
"#webView[platfrom=ios]":{
	willHandleTouches : false
}
"#buttonView[formFactor=handheld]":{
	width : "100%",
	height : 40,
	backgroundColor : "transparent",
	bottom : 5,
}
"#buttonView[formFactor=tablet]":{
	width : "100%",
	height : 55,
	backgroundColor : "transparent",
	bottom : 5,
}
"#lblCurrentPage[formFactor=handheld]":{
	width : Titanium.UI.SIZE,
	height : Titanium.UI.SIZE,
	font : Alloy.Globals.path.font12,
	color : Alloy.Globals.path.navBarColor
}
"#lblCurrentPage[formFactor=tablet]":{
	width : Titanium.UI.SIZE,
	height : Titanium.UI.SIZE,
	font : Alloy.Globals.path.font18,
	color : Alloy.Globals.path.navBarColor
}
"#btnPrevious[formFactor=handheld]":{
	left : 10,
	width : 35,
	height : 35,
	image : Alloy.Globals.path.icnBack,
	backgroundColor : Alloy.Globals.path.navBarColor,
	color : "white",
	style : "plain",
	font :  Alloy.Globals.path.font12,
	visible:false
}
"#btnPrevious[formFactor=tablet]":{
	left : 10,
	width : 50,
	height : 50,
	image : Alloy.Globals.path.icnBack,
	backgroundColor : Alloy.Globals.path.navBarColor,
