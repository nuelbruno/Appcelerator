	width : 35,
	height : 35,
	image : Alloy.Globals.path.icnNext,
	backgroundColor : Alloy.Globals.path.navBarColor,
	color : "white",
	style : "plain",
	font :  Alloy.Globals.path.font12,
	visible :false
}
"#btnNext[formFactor=tablet]":{
	right : 10,
	width : 50,
	height : 50,
	image : Alloy.Globals.path.icnNext,
	backgroundColor : Alloy.Globals.path.navBarColor,
	color : "white",
	style : "plain",
	font :  Alloy.Globals.path.font12,
	visible :false
}
