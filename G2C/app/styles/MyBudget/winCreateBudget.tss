}
"#lblTotal[formFactor=tablet]":{
	font:Alloy.Globals.path.font18Bold
}
".revenueTextBoxProperty1":{
	width: 130,
	height: 30,
	borderWidth: 1,
	borderColor: '#C0C0C0',
	font:Alloy.Globals.path.font14,
	paddingLeft: 10,
	paddingRight: 10,
	color:Alloy.Globals.path.darkGrayColor,
	backgroundColor: "transparent"
}
".revenueTextBoxProperty1[formFactor=tablet]":{
	font:Alloy.Globals.path.font18,
	width: 180,
	height: 45
}
".revenueTextBoxProperty":{
	width: 100,
	height: 30,
	borderWidth: 1,
	borderColor:Alloy.Globals.path.formFieldsColor,
	font:Alloy.Globals.path.font14,
	paddingLeft: 10,
	paddingRight: 10,
	hintText: 0,
	color:Alloy.Globals.path.formFieldsColor,
	keyboardType: Titanium.UI.KEYBOARD_NUMBER_PAD,
	bubbleParent: false,
	backgroundColor: "transparent"
}
".revenueTextBoxProperty[formFactor=tablet]":{
	font:Alloy.Globals.path.font18,
	height: 45,
	width: 130
}
".lblRevenueTotal":{
	paddingLeft: 10,
	paddingRight: 10,
	font:Alloy.Globals.path.font14Bold,
	color:Alloy.Globals.path.formFieldsColor
}
