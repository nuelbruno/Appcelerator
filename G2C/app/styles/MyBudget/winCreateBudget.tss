	width: 14,
	height: 14,
	image:Alloy.Globals.path.icnPlusBlack
}
"#imgPlusBlack[formFactor=tablet]":{
	width: 22,
	height: 22
}
"#lblAddSource":{
	text:Alloy.Globals.selectedLanguage.addSourceTitle,
	color: Alloy.Globals.path.darkGrayColor,
	font: Alloy.Globals.path.font14
}
"#lblAddSource[formFactor=tablet]":{
	font: Alloy.Globals.path.font18
}
"#lblRevenueResourcesValue":{
