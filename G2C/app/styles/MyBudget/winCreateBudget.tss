}
"#txtNote":{
	top: 0,
	height: 130,
	font:Alloy.Globals.path.font14,
	color:Alloy.Globals.path.formFieldsColor,
	value:Alloy.Globals.selectedLanguage.noteHintText,
	_hintText:Alloy.Globals.selectedLanguage.noteHintText,
	//backgroundColor: Alloy.Globals.path.bgColor,
	backgroundColor: "transparent",
	suppressReturn: false
}
"#txtNote[formFactor=tablet]":{
	height: 180,
	font:Alloy.Globals.path.font18
}
//Revenue multiple
"#viewRevenueResourcesGrid":{
	layout: 'vertical'
}
