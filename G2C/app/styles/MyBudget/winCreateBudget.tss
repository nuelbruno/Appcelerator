	bottom: 35
}
".lblProperty":{
	width: '35%',
	font:Alloy.Globals.path.font14
}
".lblProperty[formFactor=tablet]":{
	font:Alloy.Globals.path.font17
}

"#imgName":{
	image:Alloy.Globals.path.icnName
}
"#lblName":{
	text:Alloy.Globals.selectedLanguage.nameTitle
}

".txtFieldProperty":{
	width: '55%',
	font:Alloy.Globals.path.font14,
	backgroundColor: "transparent"
}
".txtFieldProperty[formFactor=tablet]":{
	font:Alloy.Globals.path.font17
}

"#txtName":{
	hintText:Alloy.Globals.selectedLanguage.budgetNameHintText,
	backgroundColor: "transparent"
}

"#imgAmount":{
	image:Alloy.Globals.path.icnAmount
}
"#imgTotal":{
	image:Alloy.Globals.path.icnAmount
}
"#lblRevenueResources":{
	text:Alloy.Globals.selectedLanguage.revenueResourceTitle
}
