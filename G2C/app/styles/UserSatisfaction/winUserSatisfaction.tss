	top : 0,
	left : 0,
	right : 0,
	bottom : 0,
}


"#tableViewItems" :{
	top : 0,
	left : 0,
	right : 0,
	bottom : 55,
	separatorColor:'transparent',
}

"#row":{
	height:Titanium.UI.SIZE,
	width:'100%',
	selectedColor:'transparent',
