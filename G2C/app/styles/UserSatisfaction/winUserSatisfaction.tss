}


"#viewContents" : {
	top : 15,
	layout : "vertical",
	height:Ti.UI.SIZE,
	backgroundColor : "transparent",
//	bottom : 10,
	left : 10,
	right : 10,
}

"#lblQuestion" :{
	top : 5,
	width : "100%",
	height :Titanium.UI.SIZE,
	font : Alloy.Globals.path.font15,
	color:"black",//Alloy.Globals.path.lblBlackColor,
	touchEnabled:false
}

"#lblQuestion[formFactor=tablet]" :{
	font : Alloy.Globals.path.font20,
}


".viewProperty":{
	top : 10,
	width : "100%",
	height:35,
	bottom : 10,
	borderColor : Alloy.Globals.path.grayColor
//	backgroundColor:"green"
}
".viewProperty[formFactor=tablet]":{
	height : 45,
}

"#lblAnswer" :{
	height :"100%",
