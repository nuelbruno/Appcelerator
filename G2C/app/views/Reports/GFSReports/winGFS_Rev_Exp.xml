<Alloy>
	<Window id="winGFS_Rev_Exp" class="win">
		<View id="mainView">
			<View class="navBarBackViewProperty">
				<View id="statusBar" class="statusBarProperty"></View>
				<View class="navBarProperty">
					<View class="backView" onClick="closeWindow">
						<ImageView class="imgBackBtn"></ImageView>
					</View>
					<Label id="lblNavTitle" class="lblNavTitleProperty"></Label>
					
				</View>
			</View>
			<View id="chartView">
				<Label id="lblReprotTitle"></Label>
				<View id="normalChartView">
					<Widget src="com.alco.highcharts" id="normalChart" class="chartViewProperty" />
				</View>
			</View>
		</View>
		<View id="TableBackView" onClick="hideTableView"></View>
		<View id="slidingView">
			<View id="sectionView">
				<Label id="lblCode" class="lblSectionProperty"></Label>
				<Label id="lblDesc" class="lblSectionProperty"></Label>
				
				<Label id="lblTotal" class="lblSectionProperty"></Label>
			</View>
			<TableView id="tableView"></TableView>
		</View>
	</Window>
</Alloy>